          {
            "group": "Tools",
            "openapi": "api-reference/openapi.json",
            "pages": [
              "POST /tool",
              "POST /tool/list",
              "GET /tool/{tool_id}",
              "PATCH /tool/{tool_id}",
              "DELETE /tool/{tool_id}"
            ]
