            "openapi": "api-reference/openapi.json",
            "pages": [
              "POST /contact/create",
              "GET /contact/list"
            ]
          },
          {
            "group": "Audiences",
            "openapi": "api-reference/openapi.json",
            "pages": [
              "POST /audience/create",
