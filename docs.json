          {
            "group": "Knowledge Base",
            "openapi": "api-reference/openapi.json",
            "pages": [
              "POST /knowledge-base/create",
              "POST /knowledge-base/list",
              "GET /knowledge-base/{knowledge_base_id}"
            ]
          }
        ]
