            "openapi": "api-reference/openapi.json",
            "pages": [
              "POST /folder/create",
              "POST /folder/list",
              "POST /folder/{folder_id}",
              "POST /folder/update",
              "POST /folder/delete",
              "POST /folder/assign-agent",
              "POST /folder/{folder_id}/agents"
