  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": [
        "python",
        "curl"
      ],
      "defaults": "all",
      "prefill": true
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Book a demo",
